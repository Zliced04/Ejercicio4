package ejercicio4;

public class Ave extends Animal{
	private String tipoDePico;

	public Ave(String nombre, String familia, String color, String dieta, String vida, String tipoDePico) {
		super(nombre, familia, color, dieta, vida);
		this.tipoDePico = tipoDePico;
	}

	public void volar(){
		System.out.println("Volando!!");
	}

}
