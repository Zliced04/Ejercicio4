package ejercicio4;

public class Animal {
	private String nombre;
	private String familia;
	private String color;
	private String dieta;
	
	public Animal(String nombre, String familia, String color, String dieta) {
		this.nombre = nombre;
		this.familia = familia;
		this.color = color;
		this.dieta = dieta;
	}
	
	public void comer() {
		System.out.println("El" + nombre + "se alimenta de: " + dieta);
		
	}
	
	

}
